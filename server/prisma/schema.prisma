generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Scoreboard {
  id  Int @id @default(autoincrement())
  createdAt DateTime @default(now())
  score Int
}

model Winners {
id  Int @id @default(autoincrement())
name String
xMin Int 
xMax Int
yMin Int
yMax Int
}

model Images {
  id Int @id @default(autoincrement())
  name String
  url String
}